{{ template "layout-base.tmpl" . }}

{{ define "pageTitle" }}Spaces{{ end }}

{{ define "mainContent" }}
<main class="relative w-full h-full overflow-y-auto lg:ml-64 pb-8" x-data="spacesListComponent('{{ .user_id }}', '{{ .username }}', '{{ .forUserId }}', {{ .permissionManageSpaces }}, '{{ .wildcard_domain }}', '{{ .zone }}', {{ .permissionTransferSpaces }}, {{ .permissionShareSpaces }})">
  <div class="grid grid-cols-1 px-4 pt-6 xl:grid-cols-4 gap-2 xl:gap-4">

    <div class="mb-4 col-span-full xl:mb-2">
      <h1 class="text-xl font-semibold text-gray-900 sm:text-2xl dark:text-white">
        {{ if eq .user_id .forUserId }}
          Spaces
        {{ else }}
          Spaces for {{ .forUserUsername }}
        {{ end }}
      </h1>
    </div>

    <form class="col-span-full sm:flex justify-between">
      <div class="flex" x-data>
        <div class="w-full sm:w-auth">
          <label for="search" class="sr-only">Search</label>
          <div class="relative mt-1 sm:w-48 lg:w-64 xl:w-96 flex items-center">
            <input type="search" name="search" id="search" class="form-field bg-white grow pr-10" placeholder="Search" x-model="searchTerm" x-on:input="searchChanged">
            <div class="absolute right-2 text-sm text-gray-500 dark:text-gray-400 font-sans ml-2 text-nowrap">âŒ˜ K</div>
          </div>
        </div>

        <button @click.prevent="$refs.panel.toggle" class="text-gray-500 dark:text-gray-400 text-sm px-2.5 py-3 cursor-pointer mt-1 ml-2 hover:text-gray-700 dark:hover:text-gray-200">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4">
            <path stroke-linecap="round" stroke-linejoin="round" d="M12 3c2.755 0 5.455.232 8.083.678.533.09.917.556.917 1.096v1.044a2.25 2.25 0 0 1-.659 1.591l-5.432 5.432a2.25 2.25 0 0 0-.659 1.591v2.927a2.25 2.25 0 0 1-1.244 2.013L9.75 21v-6.568a2.25 2.25 0 0 0-.659-1.591L3.659 7.409A2.25 2.25 0 0 1 3 5.818V4.774c0-.54.384-1.006.917-1.096A48.32 48.32 0 0 1 12 3Z" />
          </svg> <span class="sr-only">Filter</span>
        </button>

        <div x-ref="panel" x-float.teleport.placement.bottom-end.flip="" @click.away="$refs.panel.close" @keydown.window.escape="$refs.panel.close" class="fixed z-50 my-1 text-base p-2 pr-4 list-none bg-white rounded-lg shadow-xl border border-gray-200 dark:bg-gray-800 dark:border-gray-700 block whitespace-nowrap" id="panel-cDV5GZ12" aria-modal="true" role="dialog" style="position: fixed; display: block; left: 492.367px; top: 109px;">

          <div x-show="canManageSpaces && !showingSpecificUser" x-cloak class="mb-4">
            <div x-data="autocompleterUser()" x-init="setDataSource('forUsersList')" data-parent-group="" data-parent-variable="forUserId" data-on-change="userChagned" @refresh-user-autocompleter.window="refresh()" class="relative" @user-selected="userChanged">
              <input x-model="search" class="form-field" type="search" placeholder="Search..." @focus="handleFocus()" @blur.debounce.200ms="showList = false" @keydown="handleKeydown($event)" x-ref="searchInput" />
              <div x-show="showList && filteredOptions.length" x-cloak class="w-full max-h-40 overflow-auto absolute z-50 my-1 text-base p-2 list-none bg-white divide-y divide-gray-100 rounded-lg shadow-xl border border-gray-200 dark:bg-gray-800 dark:border-gray-700 dark:divide-gray-600 block whitespace-nowrap">
                <ul class="list-none">
                  <template x-for="(option, index) in filteredOptions" :key="index">
                    <li @click="selectOption(option)" x-text="option.username" :class="{'bg-blue-100 dark:bg-blue-900': selectedIndex === index}" class="my-1 cursor-pointer nav-item text-sm"></li>
                  </template>
                </ul>
              </div>
            </div>
          </div>

          <div class="mb-4" :class="{'mt-4': !canManageSpaces || showingSpecificUser}">
            <label class="flex items-center cursor-pointer">
              <input type="checkbox" @change="searchChanged" class="sr-only peer" value="1" :checked="showRunningOnly" x-model="showRunningOnly">
              <div class="relative w-9 h-5 bg-gray-200 peer-focus:outline-hidden peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:rtl:after:-translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:start-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-4 after:w-4 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600 peer-checked:dark:bg-blue-600"></div>
              <span class="ms-3 text-sm font-medium text-gray-900 dark:text-gray-300">Running</span>
            </label>
          </div>

          <div class="mb-4">
            <label class="flex items-center cursor-pointer mb-4">
              <input type="checkbox" @change="searchChanged" class="sr-only peer" value="1" :checked="showLocalOnly" x-model="showLocalOnly">
              <div class="relative w-9 h-5 bg-gray-200 peer-focus:outline-hidden peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:rtl:after:-translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:start-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-4 after:w-4 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600 peer-checked:dark:bg-blue-600"></div>
              <span class="ms-3 text-sm font-medium text-gray-900 dark:text-gray-300">In This Zone</span>
            </label>
          </div>

          <div class="mb-4">
            <label class="flex items-center cursor-pointer mb-4">
              <input type="checkbox" @change="searchChanged" class="sr-only peer" value="1" :checked="showSharedOnly" x-model="showSharedOnly">
              <div class="relative w-9 h-5 bg-gray-200 peer-focus:outline-hidden peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:rtl:after:-translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:start-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-4 after:w-4 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600 peer-checked:dark:bg-blue-600"></div>
              <span class="ms-3 text-sm font-medium text-gray-900 dark:text-gray-300">Shared</span>
            </label>
          </div>
          <div class="mb-4">
            <label class="flex items-center cursor-pointer mb-4">
              <input type="checkbox" @change="searchChanged" class="sr-only peer" value="1" :checked="showSharedWithMeOnly" x-model="showSharedWithMeOnly">
              <div class="relative w-9 h-5 bg-gray-200 peer-focus:outline-hidden peer-focus:ring-4 peer-focus:ring-blue-300 dark:peer-focus:ring-blue-800 rounded-full peer dark:bg-gray-700 peer-checked:after:translate-x-full peer-checked:rtl:after:-translate-x-full peer-checked:after:border-white after:content-[''] after:absolute after:top-[2px] after:start-[2px] after:bg-white after:border-gray-300 after:border after:rounded-full after:h-4 after:w-4 after:transition-all dark:border-gray-600 peer-checked:bg-blue-600 peer-checked:dark:bg-blue-600"></div>
              <span class="ms-3 text-sm font-medium text-gray-900 dark:text-gray-300">Shared With Me</span>
            </label>
          </div>

        </div>

      </div>
      <div>
        <button @click.prevent="openTemplateSelector" type="button" class="btn-primary flex items-center mt-1">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="size-4 mr-2">
            <path fill-rule="evenodd" d="M12 3.75a.75.75 0 0 1 .75.75v6.75h6.75a.75.75 0 0 1 0 1.5h-6.75v6.75a.75.75 0 0 1-1.5 0v-6.75H4.5a.75.75 0 0 1 0-1.5h6.75V4.5a.75.75 0 0 1 .75-.75Z" clip-rule="evenodd" />
          </svg>
          New Space
        </button>
      </div>
    </form>

    <div class="p-4 mb-4 bg-white border border-gray-200 rounded-lg shadow-xs col-span-full dark:border-gray-700 sm:p-6 dark:bg-gray-800">
      {{ template "loading" . }}

      <template x-if="!loading && (!visibleSpaces && !searchTerm.length && showLocalOnly)">
        <div class="w-full max-w-lg mx-auto p-4 bg-white border border-gray-200 rounded-lg shadow-lg dark:border-gray-700 dark:bg-gray-800">
            <div class="text-center text-gray-500 dark:text-gray-400">No spaces found.<br />Get started by <button @click.prevent="openTemplateSelector" class="text-blue-500 hover:underline dark:text-blue-400">selecting a template</button>.</div>
        </div>
      </template>

      <div x-show="!loading && (visibleSpaces || searchTerm.length)" x-cloak class="relative overflow-x-auto">
        <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
          <colgroup>
            <col class="w-1/1.1 lg:w-1/3.5 xl:w-1/3" />
            <col class="w-auto" /> <!-- Services -->
            <col class="w-auto" /> <!-- Status -->
            <col class="w-auto" /> <!-- Share -->
            <col class="w-auto" /> <!-- Template -->
            <col class="w-1" />
          </colgroup>
          <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
            <tr>
              <th scope="col" class="px-4 py-3">Details</th>
              <th scope="col" class="px-4 py-3 hidden md:table-cell">Services</th>
              <th scope="col" class="px-4 py-3 hidden lg:table-cell">Status</th>
              <th scope="col" class="px-4 py-3 hidden 2xl:table-cell">Share</th>
              <th scope="col" class="px-4 py-3 hidden xl:table-cell">Template</th>
              <th scope="col" class="px-4 py-3">&nbsp;</th>
            </tr>
          </thead>
          <tbody>
            <template x-for="s in spaces" :key="s.space_id">
              <tr x-show="!s.searchHide" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600/10">

                <!-- Space Details -->
                <td class="px-4 py-3">
                  <div class="flex items-center gap-4">
                    <img x-show="s.icon_url.length && s.icon_url_exists" :src="s.icon_url" alt="Icon" class="hidden sm:block size-8" />
                    <div>
                      <div class="text-base font-semibold text-gray-900 dark:text-white" x-text="s.name"></div>

                      <!-- Show first 2 lines of description inline -->
                      <template x-if="s.description || s.note">
                        <div class="flex gap-2">

                          <!-- Only show info button if there's more than can be displayed -->
                          <button x-show="((s.description || '').split('\n').concat((s.note || '').split('\n')).filter(Boolean)).length >= 2"
                                  @click="spaceDesc.show = true; spaceDesc.space = s"
                                  class="text-gray-500 dark:text-gray-400 flex-shrink-0 cursor-pointer">
                            <svg xmlns="http://www.w3.org/2000/svg" class="size-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                            </svg>
                          </button>

                          <div class="flex-grow">
                            <div class="text-sm text-gray-500 dark:text-gray-400 line-clamp-2" x-text="s.description.trim() + (s.description.trim().length && s.note.trim().length ? '\n' : '') + s.note.trim()" style="white-space: pre-line"></div>
                          </div>
                        </div>
                      </template>

                      <div class="xl:hidden">
                        <div class="mt-2 flex flex-wrap gap-1 text-xs">
                          <template x-if="!s.is_local">
                            <span class="inline-block bg-yellow-100 text-yellow-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300 whitespace-nowrap">Remote: <span x-text="s.zone"></span></span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'manual'">
                            <span class="inline-block bg-yellow-100 text-yellow-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Manual</span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'docker'">
                            <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Docker</span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'podman'">
                            <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Podman</span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'apple'">
                            <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Apple</span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'container'">
                            <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Container</span>
                          </template>
                          <template x-if="s.is_local && s.platform === 'nomad'">
                            <span class="inline-block bg-green-100 text-green-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-green-400 border border-green-400 whitespace-nowrap">Nomad</span>
                          </template>
                        </div>
                      </div>

                      <div class="2xl:hidden">
                        <template x-if="s.shared_username != ''">
                          <span class="mt-2 text-xs inline-block bg-blue-100 text-blue-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-blue-300 border border-blue-300 whitespace-nowrap" x-text="(s.shared_user_id == forUserId ? 'Shared By: ' : 'Shared With: ') + (s.shared_user_id == forUserId ? s.username : s.shared_username)"></span>
                        </template>
                      </div>

                      <div class="xl:hidden"><div class="mt-2">Template: <span class="text-gray-700 dark:text-gray-300 text-xs" x-text="s.template_name"></span></div></div>

                      <template x-if="forUserId == ''">
                        <div class="text-xs mt-2">User: <span class="text-gray-700 dark:text-gray-300" x-text="s.username"></span></div>
                      </template>
                      <template x-if="s.is_local && s.platform == 'manual'">
                        <div class="mt-2 text-xs">
                          <div class="flex flex-nowrap text-xs px-2 py-1 bg-gray-100 dark:bg-gray-700 rounded-lg cursor-pointer w-full hover:bg-gray-200 dark:hover:bg-gray-800" @click.debounce.750="copyToClipboard(s.space_id)">
                            <div class="shrink-0 grow-0 py-1 pr-1">ID:</div>
                            <input type="text" class="shrink grow w-full p-0 text-xs bg-transparent cursor-pointer outline-0 border-0 focus:border-0 focus:ring-0" x-model="s.space_id" readonly @click.debounce.750="copyToClipboard(s.space_id)" />
                            <div class="shrink-0 grow-0 ml-1">
                              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-5 flex-shrink-0 self-end">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M15.75 17.25v3.375c0 .621-.504 1.125-1.125 1.125h-9.75a1.125 1.125 0 0 1-1.125-1.125V7.875c0-.621.504-1.125 1.125-1.125H6.75a9.06 9.06 0 0 1 1.5.124m7.5 10.376h3.375c.621 0 1.125-.504 1.125-1.125V11.25c0-4.46-3.243-8.161-7.5-8.876a9.06 9.06 0 0 0-1.5-.124H9.375c-.621 0-1.125.504-1.125 1.125v3.5m7.5 10.375H9.375a1.125 1.125 0 0 1-1.125-1.125v-9.25m12 6.625v-1.875a3.375 3.375 0 0 0-3.375-3.375h-1.5a1.125 1.125 0 0 1-1.125-1.125v-1.5a3.375 3.375 0 0 0-3.375-3.375H9.75" />
                              </svg>
                            </div>
                          </div>
                        </div>
                      </template>

                      <!-- Status & Uptime (mobile only) -->
                      <div class="lg:hidden flex flex-col gap-1 items-start mt-2">
                        <span x-show="s.is_deployed && !s.is_pending" class="inline-block w-fit bg-green-100 text-green-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-green-400 border border-green-400">Running</span>
                        <span x-show="!s.is_deployed && s.is_pending" class="inline-block w-fit bg-yellow-100 text-yellow-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Starting</span>
                        <span x-show="s.is_deployed && s.is_pending" class="inline-block w-fit bg-yellow-100 text-yellow-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Stopping</span>
                        <span x-show="s.is_deleting" class="inline-block w-fit bg-red-100 text-red-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-red-400 border border-red-400">Deleting</span>
                        <span x-show="s.is_local && s.update_available && !s.is_pending" class="inline-block w-fit bg-indigo-100 text-indigo-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 text-nowrap">Update Available</span>
                      </div>
                      <div class="text-xs text-gray-500 dark:text-gray-400 mt-1 lg:hidden">Uptime: <span class="text-gray-700 dark:text-gray-300" x-text="s.is_deployed ? s.uptime : '-'"></span></div>

                      <!-- Services (mobile only) -->
                      <div x-show="s.is_local && s.is_deployed && !s.is_pending && (s.user_id == '{{ .user_id }}' || s.shared_user_id == '{{ .user_id }}')" class="flex flex-wrap items-center gap-1 md:hidden" x-data>
                        {{ if .permissionUseVNC }}
                        <!-- VNC -->
                        <div x-data x-show="s.has_http_vnc" x-cloak>
                          <button @click="openWindowForVNC(s.username, s.space_id, s.name)" @mouseenter="$refs.tooltip.open" @mouseleave="$refs.tooltip.close" class="cursor-pointer text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 rounded-lg p-1.5">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6"><path stroke-linecap="round" stroke-linejoin="round" d="M9 17.25v1.007a3 3 0 0 1-.879 2.122L7.5 21h9l-.621-.621A3 3 0 0 1 15 18.257V17.25m6-12V15a2.25 2.25 0 0 1-2.25 2.25H5.25A2.25 2.25 0 0 1 3 15V5.25m18 0A2.25 2.25 0 0 0 18.75 3H5.25A2.25 2.25 0 0 0 3 5.25m18 0V12a2.25 2.25 0 0 1-2.25 2.25H5.25A2.25 2.25 0 0 1 3 12V5.25" /></svg>
                            <span class="sr-only">Desktop</span>
                          </button>
                          <div x-ref="tooltip" x-float.placement.bottom.flip class="absolute z-50 my-1 px-2 py-1 text-xs text-white bg-gray-600 dark:bg-gray-900 rounded shadow-xs" role="tooltip" x-cloak>Desktop</div>
                        </div>
                        {{ end }}
                        <!-- Code Server -->
                        {{ if .permissionUseCodeServer }}
                        <div x-data x-show="s.has_code_server" x-cloak>
                          <button @click="openCodeServer(s.space_id)" @mouseenter="$refs.tooltip.open" @mouseleave="$refs.tooltip.close" class="cursor-pointer text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 rounded-lg p-1.5">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6"><path stroke-linecap="round" stroke-linejoin="round" d="M17.25 6.75 22.5 12l-5.25 5.25m-10.5 0L1.5 12l5.25-5.25m7.5-3-4.5 16.5" /></svg>
                            <span class="sr-only">Code Server</span>
                          </button>
                          <div x-ref="tooltip" x-float.placement.bottom.flip class="absolute z-50 my-1 px-2 py-1 text-xs text-white bg-gray-600 dark:bg-gray-900 rounded shadow-xs" role="tooltip" x-cloak>Code Server</div>
                        </div>
                        {{ end }}
                        <!-- VSCode Tunnel Connect -->
                        {{ if .permissionUseVSCodeTunnel }}
                        <div x-data x-show="s.has_vscode_tunnel && !s.vscode_tunnel_name.length" x-cloak>
                          <button @click="openTerminalTunnel(s.space_id)" @mouseenter="$refs.tooltip.open" @mouseleave="$refs.tooltip.close" class="cursor-pointer text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 rounded-lg p-1.5">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6 text-red-500 dark:text-red-400"><path stroke-linecap="round" stroke-linejoin="round" d="M17.25 6.75 22.5 12l-5.25 5.25m-10.5 0L1.5 12l5.25-5.25m7.5-3-4.5 16.5" /></svg>
                            <span class="sr-only">Connect VSCode Tunnel</span>
                          </button>
                          <div x-ref="tooltip" x-float.placement.bottom.flip class="absolute z-50 my-1 px-2 py-1 text-xs text-white bg-gray-600 dark:bg-gray-900 rounded shadow-xs" role="tooltip" x-cloak>Connect VSCode Tunnel</div>
                        </div>
                        {{ end }}
                        <!-- VSCode Tunnel Open -->
                        {{ if .permissionUseVSCodeTunnel }}
                        <div x-data x-show="s.has_vscode_tunnel && s.vscode_tunnel_name.length" x-cloak>
                          <button @click="openVSCodeDev(s.vscode_tunnel_name)" @mouseenter="$refs.tooltip.open" @mouseleave="$refs.tooltip.close" class="cursor-pointer text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 rounded-lg p-1.5">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6"><path stroke-linecap="round" stroke-linejoin="round" d="M17.25 6.75 22.5 12l-5.25 5.25m-10.5 0L1.5 12l5.25-5.25m7.5-3-4.5 16.5" /></svg>
                            <span class="sr-only">VSCode</span>
                          </button>
                          <div x-ref="tooltip" x-float.placement.bottom.flip class="absolute z-50 my-1 px-2 py-1 text-xs text-white bg-gray-600 dark:bg-gray-900 rounded shadow-xs" role="tooltip" x-cloak>VSCode</div>
                        </div>
                        {{ end }}
                        <!-- Terminal -->
                        {{ if .permissionUseWebTerminal }}
                        <div x-data x-show="s.has_terminal" x-cloak>
                          <button @click="openTerminal(s.space_id)" @mouseenter="$refs.tooltip.open" @mouseleave="$refs.tooltip.close" class="cursor-pointer text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 rounded-lg p-1.5">
