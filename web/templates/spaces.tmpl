                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6"><path stroke-linecap="round" stroke-linejoin="round" d="M12 12.75c1.148 0 2.278.08 3.383.237 1.037.146 1.866.966 1.866 2.013 0 3.728-2.35 6.75-5.25 6.75S6.75 18.728 6.75 15c0-1.046.83-1.867 1.866-2.013A24.204 24.204 0 0 1 12 12.75Zm0 0c2.883 0 5.647.508 8.207 1.44a23.91 23.91 0 0 1-1.152 6.06M12 12.75c-2.883 0-5.647.508-8.208 1.44.125 2.104.52 4.136 1.153 6.06M12 12.75a2.25 2.25 0 0 0 2.248-2.354M12 12.75a2.25 2.25 0 0 1-2.248-2.354M12 8.25c.995 0 1.971-.08 2.922-.236.403-.066.74-.358.795-.762a3.778 3.778 0 0 0-.399-2.25M12 8.25c-.995 0-1.97-.08-2.922-.236-.402-.066-.74-.358-.795-.762a3.734 3.734 0 0 1 .4-2.253M12 8.25a2.25 2.25 0 0 0-2.248 2.146M12 8.25a2.25 2.25 0 0 1 2.248 2.146M8.683 5a6.032 6.032 0 0 1-1.155-1.002c.07-.63.27-1.222.574-1.747m.581 2.749A3.75 3.75 0 0 1 15.318 5m0 0c.427-.283.815-.62 1.155-.999a4.471 4.471 0 0 0-.575-1.752M4.921 6a24.048 24.048 0 0 0-.392 3.314c1.668.546 3.416.914 5.223 1.082M19.08 6c.205 1.08.337 2.187.392 3.314a23.882 23.882 0 0 1-5.223 1.082" /></svg>
                        <span class="sr-only">Logs</span>
                      </button>
                      <div x-ref="tooltip" x-float.placement.bottom.flip class="absolute z-50 my-1 px-2 py-1 text-xs text-white bg-gray-600 dark:bg-gray-900 rounded shadow-xs" role="tooltip" x-cloak>Logs</div>
                    </div>
                    {{ end }}
                  </div>
                </td>

                <!-- Status & Uptime (desktop only) -->
                <td class="px-4 py-3 hidden lg:table-cell">
                  <div class="flex flex-col gap-1 items-start">
                    <span x-show="!s.is_deployed && !s.is_pending && !s.is_deleting" class="text-xs text-gray-500 dark:text-gray-400">-</span>
                    <span x-show="s.is_deployed && !s.is_pending" class="inline-block w-fit bg-green-100 text-green-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-green-400 border border-green-400">Running</span>
                    <span x-show="!s.is_deployed && s.is_pending" class="inline-block w-fit bg-yellow-100 text-yellow-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Starting</span>
                    <span x-show="s.is_deployed && s.is_pending" class="inline-block w-fit bg-yellow-100 text-yellow-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Stopping</span>
                    <span x-show="s.is_deleting" class="inline-block w-fit bg-red-100 text-red-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-red-400 border border-red-400">Deleting</span>
                    <span x-show="s.is_local && s.update_available && !s.is_pending" class="inline-block w-fit bg-indigo-100 text-indigo-800 text-xs font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 text-nowrap">Update Available</span>
                  </div>
                  <div class="text-xs text-gray-500 dark:text-gray-400 mt-1">Uptime: <span class="text-gray-700 dark:text-gray-300 whitespace-nowrap" x-text="s.is_deployed ? s.uptime : '-'"></span></div>
                </td>

                <!-- Share (desktop only) -->
                <td class="px-4 py-3 hidden 2xl:table-cell text-xs">
                  <template x-if="s.shared_username != ''">
                    <span class="inline-block bg-blue-100 text-blue-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-blue-300 border border-blue-300 whitespace-nowrap" x-text="(s.shared_user_id == forUserId ? 'Shared By: ' : 'Shared With: ') + (s.shared_user_id == forUserId ? s.username : s.shared_username)"></span>
                  </template>
                  <template x-if="s.shared_username == ''">
                    <div class="text-gray-500 dark:text-gray-400">-</div>
                  </template>
                </td>

                <!-- Template / Type (desktop only) -->
                <td class="px-4 py-3 hidden xl:table-cell">
                  <div class="flex flex-wrap gap-1 text-xs">
                    <template x-if="!s.is_local">
                      <span class="inline-block bg-yellow-100 text-yellow-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300 whitespace-nowrap">Remote: <span x-text="s.zone"></span></span>
                    </template>
                    <template x-if="s.is_local && s.platform == 'manual'">
                      <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Manual</span>
                    </template>
                    <template x-if="s.is_local && s.platform == 'docker'">
                      <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Docker</span>
                    </template>
                    <template x-if="s.is_local && s.platform == 'podman'">
                      <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Podman</span>
                    </template>
                    <template x-if="s.is_local && s.platform === 'apple'">
                      <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Apple</span>
                    </template>
                    <template x-if="s.is_local && s.platform === 'container'">
                      <span class="inline-block bg-indigo-100 text-indigo-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400 whitespace-nowrap">Container</span>
                    </template>
                    <template x-if="s.is_local && s.platform === 'nomad'">
                      <span class="inline-block bg-green-100 text-green-800 font-medium px-2 py-0.5 rounded-sm dark:bg-gray-700 dark:text-green-400 border border-green-400 whitespace-nowrap">Nomad</span>
                    </template>
                  </div>
                  <div class="mt-2 text-xs text-gray-500 dark:text-gray-400">
                    <div>Template: <span class="text-gray-700 dark:text-gray-300" x-text="s.template_name"></span></div>
                  </div>
                </td>

                <!-- More Actions -->
                <td class="px-4 py-3 text-right">
                  <div x-data x-show="s.is_local" x-cloak>
                    <button @click="$refs.panel.toggle" class="text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 focus:outline-hidden rounded-lg text-sm p-2.5 cursor-pointer">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="size-5">
                        <path fill-rule="evenodd" d="M10.5 6a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0Zm0 6a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0Zm0 6a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0Z" clip-rule="evenodd" />
                      </svg><span class="sr-only">More</span>
                    </button>

                    <div x-ref="panel" x-float.teleport.placement.bottom-end.flip @click.away="$refs.panel.close" @keydown.window.escape="$refs.panel.close" class="fixed z-50 my-1 text-base p-2 list-none bg-white rounded-lg shadow-xl border border-gray-200 dark:bg-gray-800 dark:border-gray-700 dark:divide-gray-600 block whitespace-nowrap" x-cloak>
                      <button x-show="s.is_local && !s.is_deployed && !s.is_pending && s.platform != 'manual'" @click="$refs.panel.close; startSpace(s.space_id); s.is_pending = true" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
                          <path stroke-linecap="round" stroke-linejoin="round" d="M15.91 11.672a.375.375 0 0 1 0 .656l-5.603 3.113a.375.375 0 0 1-.557-.328V8.887c0-.286.307-.466.557-.327l5.603 3.112Z" />
                        </svg> Start
                      </button>

                      {{ if .permissionUseVSCodeTunnel }}
                      <button x-show="s.user_id == forUserId && s.is_local && s.is_deployed && !s.is_pending && s.user_id == '{{ .user_id }}' && s.has_vscode_tunnel && s.vscode_tunnel_name.length" @click="$refs.panel.close; openTerminalTunnel(s.space_id)" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M17.25 6.75 22.5 12l-5.25 5.25m-10.5 0L1.5 12l5.25-5.25m7.5-3-4.5 16.5" />
                        </svg> VSCode Tunnel
                      </button>
                      {{ end }}

                      <button x-show="s.user_id == forUserId && s.is_local && !s.is_pending" @click="$refs.panel.close; editSpace(s.space_id)" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="m16.862 4.487 1.687-1.688a1.875 1.875 0 1 1 2.652 2.652L10.582 16.07a4.5 4.5 0 0 1-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 0 1 1.13-1.897l8.932-8.931Zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0 1 15.75 21H5.25A2.25 2.25 0 0 1 3 18.75V8.25A2.25 2.25 0 0 1 5.25 6H10" />
                        </svg> Edit
                      </button>

                      {{ if .permissionUseSSH }}
                      <button x-show="s.is_local && s.is_deployed && !s.is_pending && (s.user_id == '{{ .user_id }}' || s.shared_user_id == '{{ .user_id }}') && s.has_ssh" @click="$refs.panel2.toggle" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="size-4 mr-2">
                          <path fill-rule="evenodd" d="M2.25 6a3 3 0 0 1 3-3h13.5a3 3 0 0 1 3 3v12a3 3 0 0 1-3 3H5.25a3 3 0 0 1-3-3V6Zm3.97.97a.75.75 0 0 1 1.06 0l2.25 2.25a.75.75 0 0 1 0 1.06l-2.25 2.25a.75.75 0 0 1-1.06-1.06l1.72-1.72-1.72-1.72a.75.75 0 0 1 0-1.06Zm4.28 4.28a.75.75 0 0 0 0 1.5h3a.75.75 0 0 0 0-1.5h-3Z" clip-rule="evenodd" />
                        </svg> SSH Info
                      </button>
                      {{ end }}

                      {{ if and .permissionShareSpaces (not .isLeafNode) }}
                      <button x-cloak x-show="s.shared_user_id == '' && s.is_local" @click="$refs.panel.close; chooseUser.show = true; chooseUser.isShare = true; chooseUser.invalidUser = false; chooseUser.space = s" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M7.217 10.907a2.25 2.25 0 1 0 0 2.186m0-2.186c.18.324.283.696.283 1.093s-.103.77-.283 1.093m0-2.186 9.566-5.314m-9.566 7.5 9.566 5.314m0 0a2.25 2.25 0 1 0 3.935 2.186 2.25 2.25 0 0 0-3.935-2.186Zm0-12.814a2.25 2.25 0 1 0 3.933-2.185 2.25 2.25 0 0 0-3.933 2.185Z" />
                        </svg> Share
                      </button>
                      {{ end }}
                      <button x-show="s.shared_user_id != '' && s.is_local" @click="$refs.panel.close; ceaseShareConfirm.show = true; ceaseShareConfirm.space = s" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M7.217 10.907a2.25 2.25 0 1 0 0 2.186m0-2.186c.18.324.283.696.283 1.093s-.103.77-.283 1.093m0-2.186 9.566-5.314m-9.566 7.5 9.566 5.314m0 0a2.25 2.25 0 1 0 3.935 2.186 2.25 2.25 0 0 0-3.935-2.186Zm0-12.814a2.25 2.25 0 1 0 3.933-2.185 2.25 2.25 0 0 0-3.933 2.185Z" />
                        </svg> Stop Sharing
                      </button>

                      <hr x-show="(s.is_deployed || s.is_pending) && s.platform != 'manual'" class="my-2" />
                      <div x-data x-show="(s.is_deployed || s.is_pending) && s.platform != 'manual'" x-cloak class="relative inline-flex w-full">
                        <!-- Main button -->
                        <button
                          @click="$refs.panel.close; doAction(s.space_id)"
                          :class="action === 'stop'
                            ? 'bg-red-700 hover:bg-red-800 dark:bg-red-600 dark:hover:bg-red-700'
                            : 'bg-yellow-700 hover:bg-yellow-800 dark:bg-yellow-600 dark:hover:bg-yellow-700'"
                          class="flex items-center p-2 text-gray-900 dark:text-gray-200 border-none my-1 cursor-pointer text-sm px-4 w-full text-white focus:z-10 rounded-l-lg"
                          role="menuitem"
                          type="button"
                        >
                          <template x-if="action === 'stop'">
                            <span class="inline-flex items-center">
                              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
                                <path stroke-linecap="round" stroke-linejoin="round" d="M9 9.563C9 9.252 9.252 9 9.563 9h4.874c.311 0 .563.252.563.563v4.874c0 .311-.252.563-.563.563H9.564A.562.562 0 0 1 9 14.437V9.564Z" />
                              </svg>
                              Stop
                            </span>
                          </template>
                          <template x-if="action === 'restart'">
                            <span class="inline-flex items-center">
                              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M16.023 9.348h4.992v-.001M2.985 19.644v-4.992m0 0h4.992m-4.993 0 3.181 3.183a8.25 8.25 0 0 0 13.803-3.7M4.031 9.865a8.25 8.25 0 0 1 13.803-3.7l3.181 3.182m0-4.991v4.99" />
                              </svg>
                              Restart
                            </span>
                          </template>
                        </button>
                        <!-- Dropdown toggle -->
                        <button
                          @click="$refs.panel2.toggle"
                          :class="action === 'stop'
                            ? 'bg-red-700 hover:bg-red-800 dark:bg-red-600 dark:hover:bg-red-700 border-white'
                            : 'bg-yellow-700 hover:bg-yellow-800 dark:bg-yellow-600 dark:hover:bg-yellow-700'"
                            class="shrink-0 grow-0 flex items-center p-2 text-gray-900 dark:text-gray-200 border-none my-1 cursor-pointer text-sm text-white focus:z-10 rounded-r-lg ml-[1px] "
                          type="button"
                          aria-label="More actions"
                        >
                          <svg class="w-4 h-4" fill="none" stroke="currentColor" stroke-width="2" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" d="M19 9l-7 7-7-7"/>
                          </svg>
                        </button>
                        <!-- Dropdown menu -->
                        <div x-ref="panel2" x-float.teleport.placement.bottom-end.flip @click.away="$refs.panel.close" @keydown.window.escape="$refs.panel.close" class="fixed z-50 my-1 text-base p-2 list-none bg-white rounded-lg shadow-xl border border-gray-200 dark:bg-gray-800 dark:border-gray-700 dark:divide-gray-600 block whitespace-nowrap" x-cloak>
                          <button @click="$refs.panel2.close; setAction('stop')" class="group nav-item text-sm px-4 w-full" role="menuitem" type="button">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                              <path stroke-linecap="round" stroke-linejoin="round" d="M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
                              <path stroke-linecap="round" stroke-linejoin="round" d="M9 9.563C9 9.252 9.252 9 9.563 9h4.874c.311 0 .563.252.563.563v4.874c0 .311-.252.563-.563.563H9.564A.562.562 0 0 1 9 14.437V9.564Z" />
                            </svg> Stop
                          </button>
                          <button @click="$refs.panel2.close; setAction('restart')" class="group nav-item text-sm px-4 w-full" role="menuitem" type="button">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                              <path stroke-linecap="round" stroke-linejoin="round" d="M16.023 9.348h4.992v-.001M2.985 19.644v-4.992m0 0h4.992m-4.993 0 3.181 3.183a8.25 8.25 0 0 0 13.803-3.7M4.031 9.865a8.25 8.25 0 0 1 13.803-3.7l3.181 3.182m0-4.991v4.99" />
                            </svg> Restart
                          </button>
                        </div>
                      </div>

                      <hr x-show="s.user_id == forUserId && s.is_local && ((!s.is_deployed && !s.is_pending) || s.platform == 'manual')" class="my-2" />
                      <button x-show="s.user_id == forUserId && s.is_local && ((!s.is_deployed && !s.is_pending) || s.platform == 'manual')" @click="$refs.panel.close; deleteConfirm.show = true; deleteConfirm.space = s" class="group nav-item text-sm px-4 w-full text-white bg-red-700 hover:bg-red-800 dark:bg-red-600 dark:hover:bg-red-700" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="m14.74 9-.346 9m-4.788 0L9.26 9m9.968-3.21c.342.052.682.107 1.022.166m-1.022-.165L18.16 19.673a2.25 2.25 0 0 1-2.244 2.077H8.084a2.25 2.25 0 0 1-2.244-2.077L4.772 5.79m14.456 0a48.108 48.108 0 0 0-3.478-.397m-12 .562c.34-.059.68-.114 1.022-.165m0 0a48.11 48.11 0 0 1 3.478-.397m7.5 0v-.916c0-1.18-.91-2.164-2.09-2.201a51.964 51.964 0 0 0-3.32 0c-1.18.037-2.09 1.022-2.09 2.201v.916m7.5 0a48.667 48.667 0 0 0-7.5 0" />
                        </svg> Delete
                      </button>

                      {{ if and .permissionTransferSpaces (not .isLeafNode) }}
                      <hr x-cloak class="my-2" x-show="s.user_id == forUserId && ((!s.is_deployed && !s.is_pending) || s.platform == 'manual')" />
                      <button x-cloak x-show="s.user_id == forUserId && ((!s.is_deployed && !s.is_pending) || s.platform == 'manual')" @click="$refs.panel.close; chooseUser.show = true; chooseUser.isShare = false; chooseUser.invalidUser = false; chooseUser.space = s" class="group nav-item text-sm px-4 w-full" role="menuitem">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4 mr-2">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M15.75 9V5.25A2.25 2.25 0 0 0 13.5 3h-6a2.25 2.25 0 0 0-2.25 2.25v13.5A2.25 2.25 0 0 0 7.5 21h6a2.25 2.25 0 0 0 2.25-2.25V15m3 0 3-3m0 0-3-3m3 3H9" />
                        </svg> Transfer To &hellip;
                      </button>
                      {{ end }}
                    </div>

                    <div x-ref="panel2" x-float.teleport.placement.left-end.flip.offset="{offset: 16}" @click.away="$refs.panel2.close" @keydown.window.escape="$refs.panel2.close" class="fixed z-50 my-1 text-base p-2 list-none bg-white divide-y divide-gray-100 rounded-lg shadow-xl border border-gray-200 dark:bg-gray-800 dark:border-gray-700 dark:divide-gray-600 block whitespace-nowrap w-full max-w-lg" x-cloak>
                      <input type="text" x-model="s.sshCmd" readonly class="form-field w-full cursor-pointer" @click="copyToClipboard(s.sshCmd); $refs.panel2.close();" />
                    </div>
                  </div>
                </td>
              </tr>
            </template>
          </tbody>
        </table>
      </div>

      <!-- Modal choose user -->
      <div x-cloak x-show="chooseUser.show" x-transition.opacity.duration.200ms x-trap.inert.noscroll="chooseUser.show" @keydown.esc.window="chooseUser.show = false" @click.self="chooseUser.show = false" class="fixed top-0 left-0 right-0 bottom-0 inset-0 z-[60] flex items-end justify-center bg-black/20 p-4 pb-8 backdrop-blur-xs sm:items-center lg:p-8" role="dialog" aria-modal="true" aria-labelledby="defaultModalTitle">
        <!-- Modal Dialog -->
        <div x-show="chooseUser.show" x-transition:enter="transition ease-out duration-200 delay-100 motion-reduce:transition-opacity" x-transition:enter-start="scale-0" x-transition:enter-end="scale-100" class="flex max-w-lg flex-col gap-4 overflow-visible rounded-lg border border-grey-200 bg-white text-neutral-600 dark:bg-gray-800 dark:border-gray-700 dark:text-neutral-300 shadow-xl">
